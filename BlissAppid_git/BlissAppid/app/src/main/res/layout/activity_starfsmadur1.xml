<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/bakgrunnur1"
    tools:context="com.example.seventeen.blissappid.starfsmadur1">

    <TextView
        android:text="@string/hello_world"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_buatiltoflu"
        android:id="@+id/buatiltoflu"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="155dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_notkunarskra"
        android:id="@+id/notkunarskra"
        android:layout_alignBottom="@+id/buatiltoflu"
        android:layout_toRightOf="@+id/buatiltoflu"
        android:layout_toEndOf="@+id/buatiltoflu"
        android:layout_marginLeft="191dp" />

</RelativeLayout>
