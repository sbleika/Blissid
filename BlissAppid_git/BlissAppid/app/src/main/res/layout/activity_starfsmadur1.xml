<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/bakgrunnur1"
    tools:context="com.example.seventeen.blissappid.starfsmadur1">

    <ImageButton
        android:src="@drawable/buatt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_buatiltoflu"
        android:id="@+id/buatiltoflu"
        android:layout_marginLeft="145dp"
        android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="152dp" />

    <ImageButton
        android:src="@drawable/nskra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_notkunarskra"
        android:id="@+id/notkunarskra"

        android:layout_marginRight="149dp"
        android:layout_alignTop="@+id/buatiltoflu"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:src="@drawable/adgerdir"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
